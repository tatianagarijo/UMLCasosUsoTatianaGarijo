@startuml
    left to right direction
    :actor1: as act1
    rectangle Sistema{
        usecase "caso de\n uso 1" as uc1
        usecase "caso de\n uso 2" as uc2
        usecase "caso de\n uso 3" as uc3
    }
    :actor2: as act2
    act1 --- uc1
    act1 --- uc2
    act1 --- uc3
    uc1 --- act2
    uc3 --- act2

@enduml